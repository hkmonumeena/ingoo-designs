<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.TenthActivity">

<ScrollView
    android:layout_width="match_parent"
    android:layout_above="@+id/rl_bottom"
    android:layout_height="match_parent">

   <LinearLayout
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:orientation="vertical">

       <RelativeLayout
           android:layout_width="match_parent"
           android:layout_marginTop="20dp"
           android:id="@+id/rlHeader"
           android:layout_height="50dp">


           <ImageView
               android:id="@+id/imageView5"
               android:layout_width="45dp"
               android:layout_height="45dp"
               android:layout_marginStart="10dp"
               android:layout_centerVertical="true"
               app:srcCompat="@drawable/blackback" />

           <TextView
               android:id="@+id/textView9"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginLeft="15dp"
               android:layout_toRightOf="@+id/imageView5"
               android:fontFamily="@font/josefinsans_semibold"
               android:text="@string/cat_gories_la_une"
               android:textColor="@color/headerlightcolor"
               android:textSize="20dp"
               android:layout_centerVertical="true"
               android:textStyle="bold"
               />




       </RelativeLayout>

       <TextView
           android:id="@+id/textView20"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginStart="20dp"
           android:fontFamily="@font/josefinsans_semibold"
           android:text="@string/d_coration"
           android:textColor="@color/pink_color"
           android:layout_marginTop="10dp"
           android:textSize="20dp"
           app:layout_constraintStart_toStartOf="parent"
           tools:layout_editor_absoluteY="16dp" />


       <LinearLayout
           android:layout_width="match_parent"
           android:weightSum="2"
           android:layout_marginTop="10dp"
           android:layout_marginLeft="10dp"
           android:layout_marginRight="10dp"
           android:layout_height="wrap_content">
           <RelativeLayout
               android:layout_width="match_parent"
               android:layout_weight="1"
               android:id="@+id/rlItems"
               android:layout_height="wrap_content">

               <androidx.cardview.widget.CardView
                   android:layout_width="wrap_content"
                   android:elevation="8dp"
                   android:id="@+id/card1"
                   android:layout_marginLeft="10dp"
                   android:layout_marginRight="10dp"
                   app:cardCornerRadius="14dp"
                   android:layout_height="120dp">

                   <ImageView
                       android:id="@+id/imageView12"
                       android:layout_width="match_parent"
                       android:layout_height="match_parent"
                       android:scaleType="fitXY"
                       app:srcCompat="@drawable/img23" />

               </androidx.cardview.widget.CardView>
               <TextView
                   android:id="@+id/textView21"
                   android:layout_width="match_parent"
                   android:layout_below="@+id/card1"
                   android:layout_height="wrap_content"
                   android:layout_marginTop="10dp"
                   android:layout_marginBottom="10dp"
                   android:fontFamily="@font/josefinsans_semibold"
                   android:text="@string/event_by_leaty"
                   android:textColor="@color/black"
                   android:textAlignment="center"
                   android:textSize="18dp"
                   />


           </RelativeLayout>
           <RelativeLayout
               android:layout_width="match_parent"
               android:layout_weight="1"
               android:layout_height="wrap_content">

               <androidx.cardview.widget.CardView
                   android:layout_width="wrap_content"
                   android:elevation="10dp"
                   android:id="@+id/card1"
                   android:layout_marginLeft="10dp"
                   android:layout_marginRight="10dp"
                   app:cardCornerRadius="14dp"
                   android:layout_height="120dp">
                   <ImageView
                       android:id="@+id/imageView12"
                       android:layout_width="match_parent"
                       android:layout_height="match_parent"
                       android:scaleType="fitXY"
                       app:srcCompat="@drawable/be" />

               </androidx.cardview.widget.CardView>
               <TextView
                   android:id="@+id/textView21"
                   android:layout_width="match_parent"
                   android:layout_below="@+id/card1"
                   android:layout_height="wrap_content"
                   android:layout_marginTop="10dp"
                   android:layout_marginBottom="10dp"
                   android:fontFamily="@font/josefinsans_semibold"
                   android:text="@string/lilou_event"
                   android:textColor="@color/black"
                   android:textAlignment="center"
                   android:textSize="18dp"
                   />


           </RelativeLayout>






       </LinearLayout>

       <TextView
           android:id="@+id/textView20"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginStart="20dp"
           android:fontFamily="@font/josefinsans_semibold"
           android:text="@string/gateaux"
           android:textColor="@color/pink_color"
           android:layout_marginTop="10dp"
           android:textSize="20dp"
           app:layout_constraintStart_toStartOf="parent"
           tools:layout_editor_absoluteY="16dp" />


       <LinearLayout
           android:layout_width="match_parent"
           android:weightSum="2"
           android:layout_marginTop="10dp"
           android:layout_marginLeft="10dp"
           android:layout_marginRight="10dp"
           android:layout_height="wrap_content">
           <RelativeLayout
               android:layout_width="match_parent"
               android:layout_weight="1"
               android:layout_height="wrap_content">

               <androidx.cardview.widget.CardView
                   android:layout_width="wrap_content"
                   android:elevation="10dp"
                   android:id="@+id/card1"
                   android:layout_marginLeft="10dp"
                   android:layout_marginRight="10dp"
                   app:cardCornerRadius="14dp"
                   android:layout_height="120dp">
                   <ImageView
                       android:id="@+id/imageView12"
                       android:layout_width="match_parent"
                       android:layout_height="match_parent"
                       android:scaleType="fitXY"
                       app:srcCompat="@drawable/img_24" />

               </androidx.cardview.widget.CardView>
               <TextView
                   android:id="@+id/textView21"
                   android:layout_width="match_parent"
                   android:layout_below="@+id/card1"
                   android:layout_height="wrap_content"
                   android:layout_marginTop="10dp"
                   android:layout_marginBottom="10dp"
                   android:fontFamily="@font/josefinsans_semibold"
                   android:text="@string/les_anges_d_emma"
                   android:textColor="@color/black"
                   android:textAlignment="center"
                   android:textSize="18dp"
                   />


           </RelativeLayout>
           <RelativeLayout
               android:layout_width="match_parent"
               android:layout_weight="1"
               android:layout_height="wrap_content">

               <androidx.cardview.widget.CardView
                   android:layout_width="wrap_content"
                   android:elevation="10dp"
                   android:id="@+id/card1"
                   android:layout_marginLeft="10dp"
                   android:layout_marginRight="10dp"
                   app:cardCornerRadius="14dp"
                   android:layout_height="120dp">
                   <ImageView
                       android:id="@+id/imageView12"
                       android:layout_width="match_parent"
                       android:layout_height="match_parent"
                       android:scaleType="fitXY"
                       app:srcCompat="@drawable/img23" />

               </androidx.cardview.widget.CardView>
               <TextView
                   android:id="@+id/textView21"
                   android:layout_width="match_parent"
                   android:layout_below="@+id/card1"
                   android:layout_height="wrap_content"
                   android:layout_marginTop="10dp"
                   android:layout_marginBottom="10dp"
                   android:fontFamily="@font/josefinsans_semibold"
                   android:text="@string/la_fine_bouch_e"
                   android:textColor="@color/black"
                   android:textAlignment="center"
                   android:textSize="18dp"
                   />


           </RelativeLayout>






       </LinearLayout>

       <TextView
           android:id="@+id/textView20"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginStart="20dp"
           android:fontFamily="@font/josefinsans_semibold"
           android:text="@string/salles_de_mariage"
           android:textColor="@color/pink_color"
           android:layout_marginTop="10dp"
           android:textSize="20dp"
           app:layout_constraintStart_toStartOf="parent"
           tools:layout_editor_absoluteY="16dp" />


       <LinearLayout
           android:layout_width="match_parent"
           android:weightSum="2"
           android:layout_marginTop="10dp"
           android:layout_marginLeft="10dp"
           android:layout_marginRight="10dp"
           android:layout_height="wrap_content">
           <RelativeLayout
               android:layout_width="match_parent"
               android:layout_weight="1"
               android:layout_height="wrap_content">

               <androidx.cardview.widget.CardView
                   android:layout_width="wrap_content"
                   android:elevation="10dp"
                   android:id="@+id/card1"
                   android:layout_marginLeft="10dp"
                   android:layout_marginRight="10dp"
                   app:cardCornerRadius="14dp"
                   android:layout_height="120dp">
                   <ImageView
                       android:id="@+id/imageView12"
                       android:layout_width="match_parent"
                       android:layout_height="match_parent"
                       android:scaleType="fitXY"
                       android:scaleY="1.2"
                       android:scaleX="1.2"
                       app:srcCompat="@drawable/img23" />

               </androidx.cardview.widget.CardView>
               <TextView
                   android:id="@+id/textView21"
                   android:layout_width="match_parent"
                   android:layout_below="@+id/card1"
                   android:layout_height="wrap_content"
                   android:layout_marginTop="10dp"
                   android:layout_marginBottom="10dp"
                   android:fontFamily="@font/josefinsans_semibold"
                   android:text="@string/salles_de_merrige"
                   android:textColor="@color/black"
                   android:textAlignment="center"
                   android:textSize="18dp"
                   />


           </RelativeLayout>
           <RelativeLayout
               android:layout_width="match_parent"
               android:layout_weight="1"
               android:layout_height="wrap_content">

               <androidx.cardview.widget.CardView
                   android:layout_width="wrap_content"
                   android:elevation="10dp"
                   android:id="@+id/card1"
                   android:layout_marginLeft="10dp"
                   android:layout_marginRight="10dp"
                   app:cardCornerRadius="14dp"
                   android:layout_height="120dp">
                   <ImageView
                       android:id="@+id/imageView12"
                       android:layout_width="match_parent"
                       android:layout_height="match_parent"
                       android:scaleType="fitXY"
                       android:scaleX="1.2"
                       android:scaleY="1.2"
                       app:srcCompat="@drawable/img23" />

               </androidx.cardview.widget.CardView>
               <TextView
                   android:id="@+id/textView21"
                   android:layout_width="match_parent"
                   android:layout_below="@+id/card1"
                   android:layout_height="wrap_content"
                   android:layout_marginTop="10dp"
                   android:layout_marginBottom="10dp"
                   android:fontFamily="@font/josefinsans_semibold"
                   android:text="@string/lo_fine_bouchee"
                   android:textColor="@color/black"
                   android:textAlignment="center"
                   android:textSize="18dp"
                   />


           </RelativeLayout>






       </LinearLayout>








   </LinearLayout>




</ScrollView>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_alignParentBottom="true"
        android:weightSum="5"
        android:id="@+id/rl_bottom"
        android:gravity="center"
        android:layout_height="wrap_content">


        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:padding="4dp"

                android:src="@drawable/homegrey" />
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:padding="4dp"
                android:visibility="gone"
                android:src="@drawable/home"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:padding="6dp"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:padding="5dp"
                android:src="@drawable/menugrey"/>
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:padding="4dp"
                android:visibility="gone"
                android:src="@drawable/menublue"/>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:padding="4dp"
                android:src="@drawable/msggrey" />
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:padding="4dp"
                android:visibility="gone"
                android:src="@drawable/msgblue"/>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:padding="4dp"
                android:src="@drawable/notigrey"/>
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:padding="4dp"
                android:visibility="gone"
                android:src="@drawable/noti"/>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:padding="4dp"
                android:src="@drawable/usergrey" />
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:padding="4dp"
                android:visibility="gone"
                android:src="@drawable/user"/>

        </RelativeLayout>


    </LinearLayout>



</RelativeLayout>